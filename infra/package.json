{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "deploy": "cross-env ENVIRONMENT=dev cdk deploy --vv $npm_config_stacks --require-approval never",
    "synth": "cross-env ENVIRONMENT=dev cdk synth --no-staging $npm_config_stacks > template.yaml"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "aws-cdk": "2.34.2",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.34.2",
    "constructs": "^10.0.0",
    "cross-env": "^7.0.3",
    "node-env": "^0.1.6",
    "source-map-support": "^0.5.21"
  }
}
